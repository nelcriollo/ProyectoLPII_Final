package log.ministerio.fabrica;

public class Menu {
	private int cod_menu;
	 private String des_menu,url_menu;
	 
	public Menu(int cod_menu, String des_menu, String url_menu) {
		super();
		this.cod_menu = cod_menu;
		this.des_menu = des_menu;
		this.url_menu = url_menu;
	}

	public int getCod_menu() {
		return cod_menu;
	}

	public void setCod_menu(int cod_menu) {
		this.cod_menu = cod_menu;
	}

	public String getDes_menu() {
		return des_menu;
	}

	public void setDes_menu(String des_menu) {
		this.des_menu = des_menu;
	}

	public String getUrl_menu() {
		return url_menu;
	}

	public void setUrl_menu(String url_menu) {
		this.url_menu = url_menu;
	}
	 
	 

}
